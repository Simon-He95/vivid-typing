{
  "name": "vivid-typing",
  "type": "module",
  "version": "1.1.48",
  "packageManager": "pnpm@10.11.1",
  "author": "Simon He",
  "license": "MIT",
  "homepage": "https://vivid-typing.netlify.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/vivid-typing.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vivid-typing/issues"
  },
  "keywords": [
    "vivid-typing",
    "typing",
    "vue"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "config": {
    "failOnWarn": false
  },
  "scripts": {
    "build": "tsup",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "preview": "pnpm -C playground run preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest --dir test run",
    "test:watch": "vitest --dir test",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "release": "npm run build && bumpp --commit --tag --push && pnpm publish"
  },
  "peerDependencies": {
    "lazy-js-utils": "^0.1.13",
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@antfu/ni": "^0.14.0",
    "@iconify-json/carbon": "^1.2.9",
    "@types/node": "^17.0.45",
    "@vitejs/plugin-vue": "^2.3.4",
    "bumpp": "^7.2.0",
    "eslint": "^9.28.0",
    "lazy-js-utils": "^0.1.44",
    "tsup": "^8.5.0",
    "typescript": "^4.9.5",
    "vite": "^6.3.5",
    "vitest": "^0.7.13",
    "vivid-typing": "workspace:^1.1.36",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "vue-tsc": "^0.33.9"
  }
}
