{
  "name": "vivid-typing",
  "version": "1.1.27",
  "packageManager": "pnpm@6.32.3",
  "keywords": [
    "vivid-typing",
    "typing",
    "vue"
  ],
  "license": "MIT",
  "author": "Simon He",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/vivid-typing.git"
  },
  "homepage": "http://vivid-typing.hejian.club/",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/Simon-He95/vivid-typing/issues"
  },
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "pkgroll --watch",
    "play": "npm -C playground run dev",
    "play:build": "npm -C playground run build",
    "preview": "npm -C playground run preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest --dir test",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.1",
    "lazy-js-utils": "^0.0.5",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@antfu/ni": "^0.14.0",
    "@iconify-json/carbon": "^1.1.2",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.4",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "esno": "^0.16.3",
    "pkgroll": "^1.3.1",
    "pnpm": "^6.32.3",
    "typescript": "^4.6.2",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-page": "0.0.0-alpha.0",
    "vite-plugin-pages": "^0.22.0",
    "vitest": "^0.7.0",
    "vivid-typing": "workspace:1.1.26",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.33.2"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
